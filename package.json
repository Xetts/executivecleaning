{
  "name": "executivecleaning",
  "version": "1.0.0",
  "description": "This is a website for a house cleaning service",
  "main": "index.html",
  "scripts": {
    "lite": "lite-server",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "parallelshell": "3.0.1"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
